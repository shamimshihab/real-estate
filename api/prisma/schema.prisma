generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User{

  id String @id @default(auto()) @map("_id") @db.ObjectId
  username String   @unique
  email String @unique
 
  password String
  avater String ?
  createdAt DateTime @default(now())

}
